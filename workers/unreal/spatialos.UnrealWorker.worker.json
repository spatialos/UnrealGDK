{
  "build": {
    "tasks_filename": "Game/Intermediate/Improbable/spatialos.unreal.worker.build.json",
    "generated_build_scripts_type": "unreal"
  },
  "bridge": {
    "worker_attribute_set": {
      "attributes": [
        "UnrealWorker"
      ]
    },
    "entity_interest": {
      "range_entity_interest": {
        "radius": 0
      }
    },
    "streaming_query": [
      {
        "global_component_streaming_query": {
          "component_name": "improbable.unreal.UnrealLevel"
        }
      }
    ],
    "component_delivery": {
      "default": "RELIABLE_ORDERED",
      "checkout_all_initially": true
    }
  },
  "managed": {
    "windows": {
      "artifact_name": "UnrealEditor@Windows.zip",
      "command": "StartServer.bat",
      "arguments": [
        "ThirdPersonExampleMap",
        "-server",
        "-stdout",
        "-nowrite",
        "-unattended",
        "-nologtimes",
        "-nopause",
        "-messaging",
        "-SaveToUserDir",
        "+workerType",
        "UnrealWorker",
        "+appName",
        "${IMPROBABLE_PROJECT_NAME}",
        "+receptionistIp",
        "${IMPROBABLE_RECEPTIONIST_HOST}",
        "+receptionistPort",
        "${IMPROBABLE_RECEPTIONIST_PORT}",
        "+workerType",
        "${IMPROBABLE_WORKER_NAME}",
        "+workerId",
        "${IMPROBABLE_WORKER_ID}",
        "+linkProtocol",
        "RakNet",
        "log=${IMPROBABLE_LOG_FILE}"
      ]
    },
    "linux": {
      "artifact_name": "UnrealWorker@Linux.zip",
      "command": "StartServer.sh",
      "arguments": [
        "${IMPROBABLE_WORKER_ID}",
        "NUF",
        "ThirdPersonExampleMap",
        "+appName",
        "${IMPROBABLE_PROJECT_NAME}",
        "+receptionistIp",
        "${IMPROBABLE_RECEPTIONIST_HOST}",
        "+receptionistPort",
        "${IMPROBABLE_RECEPTIONIST_PORT}",
        "+workerType",
        "${IMPROBABLE_WORKER_NAME}",
        "+workerId",
        "${IMPROBABLE_WORKER_ID}",
        "+linkProtocol",
        "RakNet",
        "log=${IMPROBABLE_LOG_FILE}"
      ]
    }
  },
  "external": {
    "default": {
      "run_type": "EXECUTABLE",
      "windows": {
        "command": "${UNREAL_HOME}\\Engine\\Binaries\\Win64\\UE4Editor.exe",
        "arguments": [
          "${IMPROBABLE_WORKER_DIR}/Game/NUF.uproject",
          "-server",
          "-stdout",
          "-nowrite",
          "-unattended",
          "-nologtimes",
          "-nopause",
          "-messaging",
          "-SaveToUserDir",
          "+workerType",
          "UnrealWorker",
          "log=UnrealWorker.log"
        ]
      }
    }
  }
}
